local MODE;local -i f;for((;$#>0;))do case "$1" in -f)f=1;;+f)f=;;-m*)if [[ ${#1} -eq 2 ]];then MODE="$2";shift;else MODE="${1:2}";fi;;+m)unset -v MODE;;-v*)local VER;if [[ ${#1} -eq 2 ]];then VER="$2";shift;else VER="${1:2}";fi;;+v)local VER=();;*[\*\?]*)local fn;for fn in ${WKD%/}/.rc/$1;do [[ f -eq 1||-e ${WKD%/}/.rc/$fn/$VER/main ]]&&eval "$(recall_ run_/unload)" "$(basename "$fn")";done;[[ ${SRC%/} == ${WKD%/} ]]||for fn in ${SRC%/}/.rc/$1;do [[ f -eq 1||-e ${SRC%/}/.rc/$fn/$VER/main ]]&&eval "$(recall_ run_/unload)" "$(basename "$fn")";done;;*)[[ f -eq 1||-e ${WKD%/}/.rc/$1/$VER/main||(${SRC%/} != ${WKD%/}&&-e ${SRC%/}/.rc/$1/$VER/main) ]]&&eval "$(recall_ run_/unload)" "$1";;esac;shift;done
